<UserControl x:Name="UserControl" x:Class="Ymf825MidiDriver.EnvelopeGraph"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Ymf825MidiDriver"
             mc:Ignorable="d" Height="80" Width="240">
    <Grid>
        <Label Content="{Binding BasicOctave, ElementName=UserControl}" HorizontalAlignment="Left" Margin="8,66,0,-2" VerticalAlignment="Top" Padding="0" Foreground="Gray" ContentStringFormat="{}{0} ms"/>
        <Polyline Points="5,5 5,80" Stroke="Gray" StrokeThickness="1" StrokeDashArray="3 2"/>
        <Polyline x:Name="AttackRateLine" Stroke="Black"/>
        <Polyline Points="5,70 235,70" Stroke="Black"/>
    </Grid>
</UserControl>
